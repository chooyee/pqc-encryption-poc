                          </div>
                          <a href="javascript:void(0)" class="btn btn-success btn-round btn-lg mt-3" id="btnGenerateCert">Generate Certificate</a>
                          <a href="javascript:void(0)" class="btn btn-info btn-round btn-lg mt-3" id="btnDownloadCert">Download Certificate</a>
                        </div>

                        <!-- Message Section -->
                        <div class="card-footer">
                          <h4 class="text-uppercase font-weight-bold">Secure Messaging</h4>
                          <input type="text" id="txtMsg" class="form-control" placeholder="Secret Message">
                          <a href="javascript:void(0)" class="btn btn-primary btn-round btn-lg mt-3" id="btnSendMsg">Send Message</a>
                        </div>

                        <!-- File Upload and Signing Section -->
                        <div class="card-footer">
                          <h4 class="text-uppercase font-weight-bold">File Operations</h4>
                          <div class="input-group">                          
                            <input id="file" name="file" type="file" class="form-control">                          
                          </div>    
                          <div class="input-group mt-3">  
                            <a href="javascript:void(0)" class="btn btn-primary btn-round btn-lg" id="btnSubmit">Upload File</a>
                            <a href="javascript:void(0)" class="btn btn-warning btn-round btn-lg" id="btnSign">Sign File</a>
                          </div>
                        </div>

                        <!-- File Verification Section -->
                        <div class="card-footer">
                          <h4 class="text-uppercase font-weight-bold">Signature Verification</h4>
                          <div class="input-group">                          
                            <input id="signedFile" name="signedFile" type="file" class="form-control" placeholder="Select signed file">                          
                          </div>    
                          <div class="input-group mt-2">                          
                            <input id="signatureFile" name="signatureFile" type="file" class="form-control" placeholder="Select signature file">                          
                          </div>
                          <div class="input-group mt-3">  
                            <a href="javascript:void(0)" class="btn btn-success btn-round btn-lg" id="btnVerify">Verify Signature</a>
                          </div>
                          <div class="mt-3">
                            <div class="verification-result" id="verificationResult"></div>
                          </div>
                        </div>
                       
