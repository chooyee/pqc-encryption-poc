                        <!-- Generate Certificate Section -->
                        <div class="card-footer">
                          <h4 class="text-uppercase font-weight-bold">Digital Certificate</h4>
                          <div class="input-group">
                            <input id="txtCertName" type="text" class="form-control" placeholder="Common Name">
                          </div>
                          <div class="input-group mt-2">
