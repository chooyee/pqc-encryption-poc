             <div class="col-md-10 ml-auto col-xl-8 mr-auto">
              <div class="mb-3">
                <h2 class="text-center">WebSocket Server Monitor</h2>
              </div>
              <!-- Nav tabs -->
              <div class="card">
                <div class="card-header">
                  <ul class="nav nav-tabs nav-tabs-primary" role="tablist">
                    <li class="nav-item">
                      <a class="nav-link active" data-toggle="tab" href="#link1" role="tablist">
                        <i class="tim-icons icon-planet"></i> PQC Server
                      </a>
                    </li>                   
                  </ul>
                </div>
                <div class="card-body">
                  <!-- Tab panes -->
                  <div class="tab-content tab-space">
                    <div class="tab-pane active" id="link1">
                      <div class="row">
                        <div class="col-md-6">
                          <div class="form-group">
                            <label>Server Status</label>
                            <div class="input-group">
                              <div class="input-group-prepend">
                                <span class="input-group-text"><i class="tim-icons icon-wifi"></i></span>
                              </div>
                              <input type="text" class="form-control" id="serverStatus" value="Online" disabled>
                            </div>
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div class="form-group">
                            <label>Active Connections</label>
                            <div class="input-group">
                              <div class="input-group-prepend">
                                <span class="input-group-text"><i class="tim-icons icon-single-02"></i></span>
                              </div>
                              <input type="text" class="form-control" id="activeConnections" value="0" disabled>
                            </div>
                          </div>
                        </div>
                      </div>
                      
                      <div class="form-group">
                        <label>Connected Clients</label>
                        <div class="table-responsive">
                          <table class="table">
                            <thead>
                              <tr>
                                <th>Socket ID</th>
                                <th>User ID</th>
                                <th>Connected At</th>
                                <th>Status</th>
                              </tr>
                            </thead>
                            <tbody id="clientsTable">
                              <!-- Client rows will be added dynamically -->
                            </tbody>
                          </table>
                        </div>
                      </div>
                      
                      <div class="form-group">
                        <label>Event Log</label>
                        <textarea id="eventLog" class="form-control" rows="12" style="background-color: #27293d; color: #fff; font-family: monospace;" readonly></textarea>
                      </div>
                      
                      <div class="form-group">
                        <div class="row">
                          <div class="col-md-6">
                            <a href="javascript:void(0)" class="btn btn-primary btn-block" id="btnStartServer">Start Server</a>
                          </div>
                          <div class="col-md-6">
                            <a href="javascript:void(0)" class="btn btn-danger btn-block" id="btnStopServer">Stop Server</a>
                          </div>
                        </div>
                      </div>
                      
                      <div class="card bg-dark">
                        <div class="card-header">
                          <h5 class="card-title">Received Files</h5>
                        </div>
                        <div class="card-body">
                          <ul id="fileList" class="list-group list-group-flush">
                            <!-- Received files will be listed here -->
                          </ul>
                        </div>
                      </div>
                    </div>                   
                  </div>
                </div>
